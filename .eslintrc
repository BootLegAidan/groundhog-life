{
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "indent": ["error", 4],
        "semi": ["error", "always"],
        "no-console": ["error", {"allow": ["warn", "error", "log", "info", "trace"]}]
    }
}